{
  "name": "@restorecommerce/ordering-srv",
  "version": "0.0.10",
  "description": "Restore Commerce microservice for managing orders",
  "main": "./lib/index.js",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/ordering-srv.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "ordering",
    "srv"
  ],
  "dependencies": {
    "@restorecommerce/acs-client": "^1.1.20",
    "@restorecommerce/chassis-srv": "^1.4.0",
    "@restorecommerce/cluster-service": "^1.0.2",
    "@restorecommerce/grpc-client": "^2.0.6",
    "@restorecommerce/kafka-client": "^1.1.4",
    "@restorecommerce/logger": "^1.2.5",
    "@restorecommerce/rc-grpc-clients": "^5.1.4",
    "@restorecommerce/resource-base-interface": "^1.4.0",
    "@restorecommerce/service-config": "^1.0.7",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@alenon/grpc-mock-server": "^3.1.3",
    "@grpc/proto-loader": "^0.7.10",
    "@types/ioredis": "^4.28.10",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.39",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "async-mutex": "^0.4.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.51.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.161.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "npm-run-all lint build:clean build:tsc",
    "build:clean": "rimraf lib",
    "build:tsc": "tsc -d",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec 'ts-node' ./src/start.ts",
    "lcov-report": "nyc report --reporter=lcov",
    "lint": "eslint src --ext .ts",
    "mocha": "cross-env NODE_ENV=test node_modules/.bin/mocha --full-trace --exit",
    "start": "node lib/start.js",
    "test": "nyc npm run mocha",
    "test-debug": "mocha -R spec ./test/*.js --inspect-brk",
    "tsctests": "tsc -d -p tsconfig.test.json"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
