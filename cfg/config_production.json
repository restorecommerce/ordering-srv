{
  "logger": {
    "elasticsearch": {
      "level": "error",
      "clientOpts": {
        "node": "http://elasticsearch:9200"
      },
      "source": "ordering-srv"
    }
  },
  "redis": {
    "host": "redis"
  },
  "database": {
    "main": {
      "host": "arangodb"
    }
  },
  "events": {
    "kafka": {
      "kafkaHost": "kafka:9092"
    }
  },
  "client": {
    "fulfillment-srv": {
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://fulfillment-srv:50067"
        ]
      }
    },
    "catalog-product-srv": {
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://catalog-srv:50068"
        ]
      }
    },
    "organization-srv": {
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://resource-srv:50053"
        ]
      }
    },
    "contact-point-type-srv": {
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://resource-srv:50053"
        ]
      }
    },
    "contact-point-srv": {
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://resource-srv:50053"
        ]
      }
    },
    "address-srv": {
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://resource-srv:50053"
        ]
      }
    },
    "country-srv": {
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://resource-srv:50053"
        ]
      }
    },
    "graph-srv": {
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://resource-srv:50053"
        ]
      }
    }
  },
  "server": {
    "transports": [
      {
        "name": "grpcOrdering",
        "provider": "grpc",
        "services": {
          "io-restorecommerce-ordering-srv": "io.restorecommerce.order.Service",
          "io-restorecommerce-ordering-reflection": "grpc.reflection.v1alpha.ServerReflection",
          "io-restorecommerce-ordering-cis": "io.restorecommerce.commandinterface.Service",
          "grpc-health-v1": "grpc.health.v1.Health"
        },
        "protos": [
          "io/restorecommerce/order.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "io/restorecommerce/commandinterface.proto",
          "grpc/health/v1/health.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "0.0.0.0:50051"
      }
    ]
  }
}
