{
  "logger": {
    "elasticsearch": {
      "level": "error",
      "clientOpts": {
        "node": "http://elasticsearch:9200"
      },
      "dataStream": true,
      "source": "ordering-srv"
    }
  },
  "redis": {
    "host": "redis"
  },
  "database": {
    "main": {
      "host": "arangodb"
    }
  },
  "events": {
    "kafka": {
      "kafka": {
        "brokers": [
          "kafka:9092"
        ]
      }
    }
  },
  "client": {
    "product": {
      "address": "catalog-srv:50051"
    },
    "fulfillment": {
      "address": "fulfillment-srv:50051"
    },
    "organization": {
      "address": "resource-srv:50051"
    },
    "contact-point-type": {
      "address": "resource-srv:50051"
    },
    "contact-point": {
      "address": "resource-srv:50051"
    },
    "address": {
      "address": "resource-srv:50051"
    },
    "country-srv": {
      "address": "resource-srv:50051"
    },
    "graph-srv": {
      "address": "resource-srv:50051"
    }
  },
  "server": {
    "transports": [
      {
        "name": "grpcOrdering",
        "provider": "grpc",
        "services": {
          "io-restorecommerce-ordering-srv": "io.restorecommerce.order.Service",
          "io-restorecommerce-ordering-reflection": "grpc.reflection.v1alpha.ServerReflection",
          "io-restorecommerce-ordering-cis": "io.restorecommerce.commandinterface.Service",
          "grpc-health-v1": "grpc.health.v1.Health"
        },
        "protos": [
          "io/restorecommerce/order.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "io/restorecommerce/commandinterface.proto",
          "grpc/health/v1/health.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "0.0.0.0:50051"
      }
    ]
  }
}
